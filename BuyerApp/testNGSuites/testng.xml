<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="BizupBuyerApp">
	<listeners>
		<listener class-name="Bizupautomation.testUtils.Listeners"/>
	</listeners>
	<test thread-count="5" name="InstallAndLogin">
		<classes>
			<class name="Bizupautomation.bizup.Install" />
		</classes>
	</test> 
	<test thread-count="5" name="EnquiryFlows">
		<classes>
			<class name="Bizupautomation.bizup.EnquiryFlows" />
		</classes>
	</test>
	<test thread-count="5" name="SaveFlow">
		<classes>
			<class name="Bizupautomation.bizup.Save" />
		</classes>
	</test>
	<test thread-count="5" name="Agent">
		<classes>
			<class name="Bizupautomation.bizup.Agent" />
		</classes>
	</test>
	<test thread-count="5" name="SellerJourneyFlow">
		<classes>
			<class name="Bizupautomation.bizup.SellerJourney" />
		</classes>
	</test>
	<test thread-count="5" name="SuperSeller">
		<classes>
			<class name="Bizupautomation.bizup.Super" />
		</classes>
	</test>
</suite> 
